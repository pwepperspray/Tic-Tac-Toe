#include <stdio.h>

char grid[3][3];

void set_up_grid(){
    for(int i = 0; i < 3; i++){
        for(int j = 0; j <3; j++){
            grid[i][j] = 'a';
        }
    }
}

void display(){
    for(int i = 0; i < 3; i++){
        printf("%3c%3c%3c%3c%3c\n", grid[i][0], '|', grid[i][1], '|', grid[i][2]);
        if(i < 2){
            printf("-----------------\n");
        } 
    }
}

int main(){
    set_up_grid();
    display();
    return 0;
}


